  "devDependencies": {
    "xo": "^0.24.0"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "indent": [
        "error",
        2,
        {
          "ignoredNodes": ["ConditionalExpression"]
        }
      ],
      "operator-linebreak": [
        "error",
        "after",
        {
          "overrides": {
            "?": "before",
            ":": "before",
            "||": "before"
          }
        }
      ]
    }
  }
