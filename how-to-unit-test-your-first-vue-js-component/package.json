  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "faucet": "0.0.1",
    "tape": "^4.10.1",
    "xo": "^0.24.0"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "indent": [
        "error",
        2,
        {
          "ignoredNodes": [
            "ConditionalExpression"
          ]
        }
      ],
      "operator-linebreak": [
        "error",
        "after",
        {
          "overrides": {
            "?": "before",
            ":": "before",
            "||": "before"
          }
        }
      ]
    }
  }
}
